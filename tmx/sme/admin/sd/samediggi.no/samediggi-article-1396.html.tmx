      <tuv xml:lang="nob">
        <seg>Sametingets plenumsmøte november/desember 2006 - Sámediggi Sametinget</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámedikki dievasčoahkkin 2006 skábmamánus/juovlamánus - Sámediggi Sametinget</seg>
      </tuv>
    </tu>
    <tu>
