    <tu>
      <tuv xml:lang="nob">
        <seg>Elsa Laula Renberg - et kvinneliv</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Elsa Laula Renberg - sámenissoniid árbi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
