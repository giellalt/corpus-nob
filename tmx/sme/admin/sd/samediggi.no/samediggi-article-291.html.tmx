    <tu>
      <tuv xml:lang="nob">
        <seg>Krav om støtte for samiske språk innen databehandling</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gáibádus sámegiela sáhttit geavahit dihtoris</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Publisert 27.04.2005</seg>
      </tuv>
      <tuv xml:lang="sme">
