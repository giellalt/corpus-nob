<tmx>
  <header segtype="sentence" o-tmf="OmegaT TMX" adminlang="en-US" srclang="nob" datatype="plaintext">
    <prop type="x-filename">sametingets_valgnemnd.html</prop>
  </header>
  <body>
    <tu>
      <tuv xml:lang="nob">
        <seg>Sametingets valgnemnd</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámedikki válganammagoddi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Valgnemnda består av 5 personer med varamedlemmer.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Válganammagottis leat 5 olbmo oktan várrelahtuiguin.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Disse skal ha stemmerett ved sametingsvalget (forskrift om sametingsvalg §18).</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dáin galgá leat vuoigatvuohta jienastit sámediggeválggas (láhkaásahus sámediggeválgga birra §18).</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Valgnemndas oppgaver er:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Válganammagotti bargun lea:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Ansvar for forberedelse og avvikling av valget</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Ovddasvástádus ráhkkanahttit ja čađahit válgga</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Sende ut kandidatbrev med informasjon om fritaksgrunner (forskriftens §16)</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sáddet evttohasreivve dieđuiguin luvvennákkaid birra (láhkaásahusa §16)</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Godkjenne listeforslag innen 01.06.21 (forskriftens §32)</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Listaevttohusaid dohkkehit 01.06.21 rádjái (láhkaásahusa §32)</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Behandle klager som gjelder godkjenning av lister (forskriftens §34)</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gieđahallat guoddalemiid mat gusket listtaid dohkkeheapmái (láhkaásahusa §34)</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Sørge for trykking av stemmesedler (forskriftens §35)</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Fuolahit ahte jienastanlihput deaddiluvvojit (láhkaásahusa §35)</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Utferdige fullmakter for valgte representanter (§72)</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Fápmudusaid ráhkadit válljejuvvon áirasiidda (§72)</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Sametingets administrasjon er valgnemndas sekretariat og sørger for de praktiske oppgavene.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámedikki hálddahus lea válganammagotti čállingoddi ja doaimmaha praktihkalaš bargguid</seg>
      </tuv>
    </tu>
  </body>
</tmx>
