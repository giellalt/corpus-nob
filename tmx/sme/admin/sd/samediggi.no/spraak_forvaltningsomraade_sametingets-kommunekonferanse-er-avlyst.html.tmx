<tmx>
  <header segtype="sentence" o-tmf="OmegaT TMX" adminlang="en-US" srclang="nob" datatype="plaintext">
    <prop type="x-filename">spraak_forvaltningsomraade_sametingets-kommunekonferanse-er-avlyst.html</prop>
  </header>
  <body>
    <tu>
      <tuv xml:lang="nob">
        <seg>Sametingets kommunekonferanse er avlyst</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Suohkankonferánsa ii lágiduvvo</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>09. februar 2015</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>09. Guovvamánnu 2015</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Kommunekonferansen er avlyst, på grunn av dårlig vær.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gielddakonferansa ii lágiduvvo heajos dálkkiid geažil.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Konferansen er på auditoriet.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Konferánsa lea auditorias.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Følg konferansen på Sametingets nett-tv:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Konferanssa sáddejuvvo njuolgga Sámedikki neahtta-tv:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>http://tv.samediggi.no</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>http://tv.samediggi.no</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Tirsdag 10. februar</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Disdaga 10. beaivi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Del 1. Tospråklighet i kommunesektoren</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>1. oassi. Guovttegielatvuohta suohkansuorggis</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>08:30 – 09:00</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Bures boahtin, Sámediggepresideanta Aili Keskitalo bokte</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>09:00 – 09:15</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dieđut Sámi giellalávdegotti birra, jođiheaddji Bård Magne Pedersen bokte</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>09:15 – 09:35</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Guovttegielatvuohta - otná dilli, hástalusat ja vuordámušat, Sámediggepresideanta Aili Keskitalo bokte</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>09:35 – 10:00</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sámedikki nuoraidlávdegotti ja vuorrasiidráđi vuordámušat suohkaniidda, SUPU ja vuorrasiidráđi bokte</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>10:00 – 10:30</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gáfeboddu ja gáhkožat, šattut</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>10:30 – 11:00</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Suohkaniid/fylkasuohkaniid vuordámušat ja jurdagat Sámedikki ja giellalávdegotti bargui, Porsáŋggu gieldda ja Snoasa suohkana vuordámušat</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>11:00 – 11:30</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Digaštallan.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>11:30 - 12:30</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Beaiveborran (Sámedikkis)</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>12:30 – 13:30</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>2. oassi. Suohkanođastus</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Registrering</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Suohkanođastus, áššedovdi bargojoavkku Lars Erik Borge bokte</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Velkommen v/Sametingspresident Aili Keskitalo</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Ráđđehusa sámi oaidnisadji suohkanođastusas, stádačálli Anne Karin Olli bokte</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Orientering om samisk språkutvalg v/leder Bård Magne Pedersen</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Áigi gažaldagaide</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Tospråklighet - dagens situasjon, utfordringer og forventinger v/ Sametingspresident Aili Keskitalo</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gáfeboddu ja gáhkožat, šattut</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Sametingets ungdomsråd og eldrerådets forventninger til kommunene, v/SUPU og eldrerådet</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Suohkanođastus - Sámi oaidninsajis, ráđđelahttu Ann-Mari Thomassen bokte</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Kaffepause m/kaker, frukt</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Sáhkavuorru suohkanođasteami birra, Fylkkamánni Finnmárkkus ja Gáivuona suohkana bokte</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Kommunenes/fylkeskommunenes forventinger og tanker til Sametingets og språkutvalgets arbeid, Innlegg v/Porsanger kommune og Snåsa kommune</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Mot váikkuha suohkanođastus sámi guovlluid giela, kultuvrra ja servodateallima ovdáneapmái.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Debatt.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Deanu gieldda ja Kárášjoga gieldda jurdagat.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Lunsj (på Sametinget)</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Digaštallan.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Del 2. Kommunereformen.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Digaštallanjođiheaddji: Loabága suohkana sátnejođiheaddji Erling Bratsberg</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>13:30 – 14:30</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Vuosttáš beaivvi loahpaheapmi.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>14:30 – 14:45</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Mállásat Scandic hoteallas Kárášjogas</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>14:45 – 15:00</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gaskavahkku guovvamánu 11. beaivi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>15:00 – 15:30</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>3. oassi. Ealáhus- ja servodatovdáneapmi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>15:30 – 16:00</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Jurdagat ivttábeaivvis, Aili Keskitalo bokte</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>16:00 – 16:30</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dieđut Dáhttu prošeavtta birra, Ragnhild Dalheim Eriksen, Sámedikki bokte</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>16:30 – 17:00</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gáfeboddu ja gáhkožat, šattut</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>17:00 - 18:00</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Ovdáneami sárgosat sámi servodagas.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>18:00</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Mot barget suohkanat mat lihkostuvvet?</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>20:00</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Knut Vareide, Telemarksforskning bokte</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Kommunereformen, v/ ekspertutvalget Lars Erik Borge.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Beaiveborran (Sámedikkis)</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Regjeringens samiske perspektiv i kommunereformen v/statssekretær Anne Karin Olli</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Digaštallan.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Tid til spørsmål</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Makkár váikkuhus lea otná ovdáneami sárgosiin ja suohkanođasteapmi sámi guovlluid ealáhus- ja servvodatovdáneapmái boahtte áiggis.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Kaffepause m/kaker, frukt</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gáfeboddu ja gáhkožat, šattut</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Kommunereformen - samisk perspektiv, v/rådsmedlem Ann-Mari Thomassen</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Čoahkkáigeassu (konferánssas ollisladjat), Sámediggepresideanta Aili Keskitalo bokte</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Innlegg om kommunereformen v/Fylkesmannen i Finnmark og Kåfjord kommune</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Loahpaheapmi ja ruoktot vuolgin</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Hvilken betydning vil kommunereformen få for utvikling av språk, kultur og samfunnsliv i samiske områder.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Bussesáhttu Scandic Kárášjoga hoteallas Leavnnja girdišilljui</seg>
      </tuv>
    </tu>
  </body>
