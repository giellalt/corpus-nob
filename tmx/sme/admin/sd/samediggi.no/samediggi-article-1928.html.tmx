      <tuv xml:lang="nob">
        <seg>Arkiv</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Arkiiva</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Begge to eller toeina</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Guovttá vai guoktin</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Publisert 18.12.2007</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Almmuhan 18.12.2007</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Begge to og ”toeina” på samisk (om to som er alene) har ulik betydning.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Guoktin ja guovttá sániin lea goabbat mearkkašupmi. Dá lea čilgehus goappá atna goasge.</seg>
      </tuv>
    </tu>
