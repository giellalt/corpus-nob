  <body>
    <tu>
      <tuv xml:lang="nob">
        <seg>Frisklivssentralen</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Álšaguovddáš-filbma</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Trenger du hjelp til å komme i gang</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dárbbašat go veahki beassat johtui</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Filmen er utarbeidet av Helsedirektoratet og bør vises til flere for enkelt og visuelt vise hva Frisklivssentralene kan tilby.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dearvvašvuođadirektoráhtta lea ráhkadan filmma, ja dan berre čájehit eanebiidda vai álkit ja visuálalaččat čájeha maid álšaguovddážat fállet.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>filmen</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Árvalusat dilálašvuođaide/báikkiide gos filmma sáhttá čájehit:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Under foredrag om frisklivssentraler</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Logaldallamiin álšaguovddážiid birra</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>For andre som jobber i kommunen</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Earáide geat barget gielddas</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>På skjermer på legekontorer, hos annet helsepersonell, apotek, bibliotek osv.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Šearpmain doavttirkantuvrrain, eará dearvvašvuođabargiid kantuvrrain, apotehkas, girjerádjosis jna.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Under ulike tilstelninger/aktiviteter</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Fáddáoahpahusas Iešguđet doaluin/aktivitehtain</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Få bedre kosthold?</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Rievdadit dábiid?</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Slutte med tobakk?</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Oažžut buoret biebmodoalu?</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Få mere overskudd!</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Heahtit duhpáhiin?</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Og fullføre den!</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Ja čađahit dan!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Snus- og røykeslutt</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Fysalaš aktivitehta</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Du får oppfølging fra helsepersonell</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Heaitit snuvssemis ja borgguheames</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Frisklivsentralen tilbyr:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dearvvašvuođabargit čuovvolit du</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Individuell veiledning</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Individuála bagadallama</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Aktivitet sammen med andre</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Aktivitehtaid ovttas earáiguin</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Kurs og informasjon</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Kurssaid ja dieđuid</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Trenger du hjelp til å komme i gang?</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Dárbbašat go veahki beassat johtui?</seg>
      </tuv>
    </tu>
