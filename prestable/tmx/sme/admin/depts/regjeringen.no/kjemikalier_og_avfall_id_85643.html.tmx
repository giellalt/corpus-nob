    <tu>
      <tuv xml:lang="nob">
        <seg>Seksjon for kjemikalier og avfall</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Kemikála- ja doapparsekšuvdna</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Vuoruhit birasbeaktilis buvttadeami ja geavaheami , dán vuolde huksema ja buktagiid .</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Arbeid med helse-og miljøfarlige kjemikalier , herunder miljøgifter i produkter , industriutslipp og forurenset grunn / sedimenter .</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Doapparunnideapmi ja ođđasitávkkástallan ( bábir , plastihkka , biilabatteriijat , orgánalaš doabbarat jna. ) Vuogádat erenoamášdoabbariid čohkkemii ja gieđahallamii .</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Váiddaáššit mat gusket erenoamášdoappar- ja boaldinrusttegiidda .</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Čorgen boares erenoamášdoappar- ja industriijanuoskkidanváttisvuođain .</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Produkt- og industrikontroll .</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Gaskariikkalaš bargu surggiid siskkobealde .</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Avfallsreduksjon og gjenvinning herunder system for innsamling og behandling av avfall og farlig avfall .</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Bargu dearvvašvuohta- ja birasváralaš kemikálaiguin , dán vuolde birasmirkkut .</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Klagesaker på områdene .</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
