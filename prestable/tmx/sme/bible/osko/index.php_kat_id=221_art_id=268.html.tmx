  <body>
    <tu>
      <tuv xml:lang="nob">
        <seg>Lær om: Vann</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Oahpa: Čáhci</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Misjonsbefalingen</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Vuolggahusgohččun</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Da trådte Jesus fram og talte til dem:</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Jesus loaiddastii sin ovdii ja sártnui sidjiide:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Meg er gitt all makt i himmel og på jord.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Munnje lea addojuvvonbuot fápmu almmis ja eatnama alde.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Gå derfor ut og gjør alle folkeslag til disipler,</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Vulget dan dihtii ja dahket buot álbmogiid máhttájeaddjin,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>idet dere døper dem til Faderens og Sønnens og Den Hellige Ånds nan</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>gásttašettiin sin Áhči ja Bártni ja Bassi Vuoiŋŋa nammii</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>og lærer dem å holde alt det jeg har befalt dere.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>ja oahpahettiin sin doallat buot dan maid mun lean gohččun din doallat.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Og se, jeg er med dere alle dager inntil erdens ende.</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>Ja gehččet, mun lean dinguin buot beiiid gitta dálá máilmmi loahpa rádjái.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>(Matt 28, 18-20)</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg>(Matt 28, 18-20)</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Utskriftsvennlig versjon</seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
    </tu>
