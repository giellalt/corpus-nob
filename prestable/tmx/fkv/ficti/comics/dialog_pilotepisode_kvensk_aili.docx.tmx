<?xml version='1.0' encoding='utf-8'?>
<tmx>
  <header segtype="sentence" o-tmf="OmegaT TMX" adminlang="en-US" srclang="nob" datatype="plaintext">
    <prop type="x-filename">dialog_pilotepisode_kvensk_aili.docx.xml</prop>
  </header>
  <body>
    <tu>
      <tuv xml:lang="nob">
        <seg>Side/rute</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>MET</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Karakter Til oversetting.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>KOULUSTA JA MINUN YSTÄVISTÄ.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>kvenske ordet Nord-Norge</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>KIITOS SIITÄ ETTE SIE SAATAT</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>og ikke nødvendigvis det kvenske ordet for Finnmark</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>PASSATA KLÄPPII SILLÄ AIKKAA KO MET SE TULLEE TYRHIIKSI, KO</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Nils</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>PÄIVÄNÄ.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>antar at dette er fast internasjonalt uttrykk.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>PIKKU MUKAN. SAATATKO SIE MUISTELLA JOTAKI SETÄ?</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>HEILÄ MOLEMILLA OON</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>KO SIE AJOIT AHMAA TAKA.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>(kaster en juicepakke i søpla).</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>JOO, KO OON TRENKI JA AUTTAA</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Nils</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Piika</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Nils</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>TOKKAA.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>På samisk brukes ordet om gutter som hjelper til med</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>TETHÄN TIÄTTÄ ETTE SE OON AJJAAT ULOS RUVAN KANS JA SENLAISTA.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>" som åpenbart er det samme ordet.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>MET OLIMA MIE ISTUIN SKUUTTERILLA JA</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Men om de</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>SÖIN NIESTAN.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>På samisk brukes ordet "</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>MATTIS SEISOI JÄÄLÄ KO HÄN</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>" som åpenbart er det samme ordet. Men om de brukes på samme måte vet jeg ikke.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>KO MIE TULIN TAKAISIN MATTIKSEN JA MUITTEN PAIMENIITTEN TYKÖ, NIIN MIE MUISTELIN HEILE ETTE SE PÄÄSI POIS.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>(hekter av hengeren)</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>NÄIN SE OON KO SETÄ PASSAA</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>(dråper på vinduet)</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>BENSIINISTASJUUNISTA JA</seg>
      </tuv>
    </tu>
  </body>
</tmx>
