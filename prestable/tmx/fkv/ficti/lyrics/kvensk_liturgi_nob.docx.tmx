<?xml version='1.0' encoding='utf-8'?>
<tmx>
  <header segtype="sentence" o-tmf="OmegaT TMX" adminlang="en-US" srclang="nob" datatype="plaintext">
    <prop type="x-filename">kvensk_liturgi_nob.docx.xml</prop>
  </header>
  <body>
    <tu>
      <tuv xml:lang="nob">
        <seg>2008 Preludium –</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Jumalan palvelus</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Aamen.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Prelu</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Inngangsbønn –</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>uti Alkurukkous</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>klokkerbønn</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Lukkarinrukkous</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Herre!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Herra!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Jeg er kommet inn i dette ditt hellige hus for å høre hva du, Gud Fader, min skaper, du Herre Jesus, min Frelser, du Hellige Ånd, min trøster i liv og død,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>sie, Herra Jeesus, minun Vapattaaja,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>vil tale til meg.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>aiot saarnata minule.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Herre!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Herra!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Lat nu opp mitt hjerte ved din Hellige Ånd,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Aukase nyt minun syðämen sinun Pyhän Hengen kautta,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>så jeg av ditt ord må lære å sørge over mine synder, og i liv og død tro på Jesus,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>niin ette mie sinun sanan kautta opin suremhaan minun synttii, ja elämässä ja kuolemassa uskomhaan Jeesuksheen,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>og hver dag forbedre meg i et hellig liv og levnet.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>ja joka päivä tekemhään parannuksen pyhässä elämässä.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Det høre og bønnhøre du ved Jesus Kristus!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Sen kuulisit ja tekisit sie Jeesuksen Kristuksen kautta.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Inngangssalme</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Alkuvirsi Alkusanat</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Inngangsor d</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Rakas seurakunta.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Kjære menighet.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Armo olkhoon teile</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Nåde være med dere og</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Ja rauha Jumalalta, meiðän Isältä,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Fred fra Gud, vår Far, og</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Ja Herralta Jeesukselta Kristukselta.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Kyrie eleison!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Kyrie eleis on</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Gud Fader miskunne deg! Kyrie eleison!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Kyrie eleison, Jumala meiðän Isä, armatta minnuu.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Herre Krist, miskunne deg!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Kriste eleison, Herra Kristus, armatta minnuu.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Kyrie eleison!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Kyrie eleis on</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Gloria</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Pappi:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Gloria Ære være Gud i det høyeste!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Kunnia olkhoon Jumalale korkkeuðessa!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Menigheten:</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Seurakunta:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Og fred på jorden, i mennesker Guds velbehag!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Ja rauha maan pääle, ja ihmisile hyvä tahto!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>tekstlesing</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Presten:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Det står skrevet</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Tekstinlukeminen</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Tekstlesningen avsluttes med:</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Oon kirjotettu...</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Slik lyder Herrens ord.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Näin oon Herran sanat.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Usk Aamen.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>tunnustus</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>La oss bekjenne vår hellige tro!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Tunnustama meiðän pyhän uskon.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>(Menigheten reiser seg)</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>(Seurakunta noussee seisomhaan.)</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Jeg tror på Gud Fader,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Mie uskon Jumalhaan,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>den allmektige,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>kaikkivalttihaasheen</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>himmelens og jordens skaper.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Taivhaan ja mailman luojhaan.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Jeg tror på Jesus Kristus,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Mie uskon Jeesuksheen Kristuksheen,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Guds enbårne Sønn, vår Herre,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Jumalan ainutsyntyisheen Poikhaan,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>som ble unnfanget ved Den Hellige Ånd, født av jomfru Maria,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>sikkiintyny Pyhän Hengen kautta, jonka Neittyt Maaria synnytti,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>pint under Pontius Pilatus,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>piinattu Pontius Pilatuksen alla,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>korsfestet, død og begravet,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>risthiinnaulittu, kuollu ja hauðattu,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>for ned til dødsriket, stod opp fra de døde tredje dag,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>meni kuolemanvaltakunthaan, nousi ylös kolmantena päivänä,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>for opp til himmelen,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>meni taivhaasheen,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>sitter ved Guds, den allmektige Faders, høyre hånd,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>istuu Jumalan kaikkivalttihaan Isän oikkeela puolela</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>skal derfra komme igjen for å dømme levende og døde.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>ja tullee takasin tuomittemhaan elläävii ja kuolheita.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Menigheten synger: Amen.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Mie uskon Pyhhään Henkheen,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Salme</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>pyhhään tavalisheen kirkhoon,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>2. 2. tekstlesing</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>pyhhiin valtakunthaan,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Paulus’ første brev til korinterne,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>synttiin antheeksi antamisheen,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Slik lyder Herrens ord</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>ruumhiin ylösnousemisheen,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Hellige oss i sannhet</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>ja ikuisheen elämhään.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Ditt ord er sannhet</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Seurakunta laulaa:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Salme</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Virsi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Preken</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Tekstinlukeminen</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Dette hellige evangelium står skrevet hos</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Tämä pyhä evankeeliumi oon kirjotettu</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Markus i</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Paavalin ensimäisessä preevissä korintilaisile, ensimäisessä luvussa:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>d</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Näin oon Herran sanat</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>et 12. kapittel (vers 28 – 34):</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Pyhitä meitä tottuuðessa</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Salme k</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>1, 4 – 9</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>s et</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Virsi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Kunngjøringer</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>enessatoista</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Forbønn</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Kuulutu</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Vær, o Gud, etter ditt løfte enkers forsvar og faderløses far.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Ole, Jumala, luppaukses jälkkheen leskiin suoja ja orpolasten isä.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Gi alle syke og sorgfulle lindring og trøst, kom nådig</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Anna kaikile sairhaila ja surulisile lievitystä ja lohðutusta,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>de anfektede til hjelp, og dem som er i dødens nød.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Tule armolisesti avuksi kiusatuile, ja heile jokka oon kuoleman häðässä.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Vær deres styrke som lider trengsel for Jesu navns skyld.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Ole heiðän voima, jokka kärsithään ahðistusta Jesuksen nimen tähðen.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Gi et fredsommelig og lykkelig samliv, godt og tjenlig vær, rettsindige og kristelige råd til all vår gjerning!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Anna rauhalisen ja onnelisen yhteiselämän, hyvän ja otolisen sään, Oikkeemielisen ja kristilisen neuvon meiðän työhöön!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Menigheten:</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Seurakunta:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Herre, hør vår bønn!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Herra, kuule meiðän rukkouksen!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>ð Aamen</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Isä mei</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Fa</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>än</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Fader vår, du som er i himmelen!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Isä meiðän, joka olet taivhaassa!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>La ditt navn holdes hellig.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Likenytkhöön sinun valtakunta</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>La ditt rike komme.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Tapattukhoon sinun tahto</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>La din vilje skje på jorden</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Niin maassa, niinko taivhaassaki</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Gi oss i dag vårt daglige brød. Forlat oss vår skyld,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Anna meile tänä päivänä meiðän jokapäiväisen leivän</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>som vi òg forlater våre skyldnere.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Ja anna meile meiðän synnit antheeksi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Led oss ikke inn i fristelse,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Ja älä johðata meitä kiussauksheen</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>men frels oss fra det oonde.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Mutta päästä meiðät pahasta</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>For riket er ditt,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Sillä sinun oon valtakunta,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>og makten og æren i evighet.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>voima, ja kunnia ijänkaikkisesti</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Amen.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Virsi</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Salme</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Siunaus</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Aamen.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Pappi:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Aamen. Aamen</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Ylistämä Herraa</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Velsignelse</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Jumalale kiitos</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>La oss prise Herren</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Halleluujaa Halleluujaa</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Gud være lovet</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Halleluujaa Pappi:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Ta i mot Herrens velsignelse:</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Ottakkaa vasthaan Herran siunauksen:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Herren velsigne deg og bevare deg!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Herra siunatkhoon sinnuu ja varjelkhoon sinnuu!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Herren la sitt ansikt lyse over deg og være deg nådig!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Herra antakhoon kasvot loistaat sinun yli ja olkhoon sinule armolinen!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Herren løfte sitt åsyn på deg og gi deg fred!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Herra kääntäkhöön kasvot sinun pääle ja antakhoon sinule rauhan!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Menigheten synger:</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Seurakunta laulaa:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Amen, amen, amen.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Halleluja Presten:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Utgangsbønn</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Loppurukkous</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Klokker</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Lukkari:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Herre!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Herra!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Jeg takker deg</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Mie kiitän sinnuu</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>fordi du har lært meg hva du vil</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Auta nyt minnuu, minun Jumala,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>ved din Hellige Ånd</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>sinun Pyhän Hengen kautta</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>for Jesu Kristi skyld,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>ja Jeesuksen Kristuksen tähðen</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>at jeg må bevare ditt ord i et rent hjerte, styrkes ved det i troen,</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>niin ette mie säilytän sinun sanan puhthaassa syðämessä, lujennun uskossa,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>forbedres i et hellig levnet</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>parannun pyhässä elämässä</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>og ha trøst av det i liv og død!</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>ja saan lohðutuksen elämässä ja kuolemassa!</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Amen i Jesu navn.</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Aamen Jeesuksen nimheen.</seg>
      </tuv>
    </tu>
  </body>
</tmx>
