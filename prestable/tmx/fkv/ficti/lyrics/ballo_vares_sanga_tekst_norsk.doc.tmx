  <body>
    <tu>
      <tuv xml:lang="nob">
        <seg>VÅRES SANGA</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>`Våres sanga'</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Lang va veien vi vandra</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Kauvoin kuljima kunnes</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Ut mot fjære og fjord</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>näimä pohjaisen maan.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Drømman våres va mange</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Toivon tunsima suuren,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Da vi reiste mot nord</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>siihen luotima ain.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Bort fra skogkledde landskap</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Vuonon, fieruan näimä,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Til en karrig natur</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>luonon jyppyräisen.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Sangan som vi har med oss</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Laulut meilä ol' myötä,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Fins i moll og i dur</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>sävelet syämen.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Tonan vi hører</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Sävelten myötä</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Har følgt oss på veian vi for</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>mie juurille palata voin, palata voin.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>På veian vi for</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Juuret oon vahvat</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Røttan e lange</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>täälä oon nyt.)</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Men livan vi lever e her</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Tänne pohjaisen maahan</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Her kor vi bor</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>aikaa siiryimä met.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Våres håp om ei fremtid</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Täälä rannoila Ruijan</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Følgte stia og spor</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>toivon tunsima met.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Og den fremmede jord</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Meän lauluissa kuulet</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Drøm og virkelighet</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>kaiken tapahtunheen.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
        <seg>Alt vi trodde og tenkte</seg>
      </tuv>
      <tuv xml:lang="fkv">
        <seg>Kaikki unet ja toivheet,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="nob">
